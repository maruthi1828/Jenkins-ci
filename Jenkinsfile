pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'This is Build Stage - First Stage 1'
            }
        }
    
      
        stage('Test') {
            steps {
                echo 'This is Test Stage - Second Stage 2'
            }
        }

        stage('Unit Test') {
            steps {
                echo 'This is Unit Test Stage - Third Stage 3'
            }
        }
    
     
      
        stage('Release') {
            steps {
                echo 'This is Release Stage - Fourth Stage 4'
            }
        }
    }
}
